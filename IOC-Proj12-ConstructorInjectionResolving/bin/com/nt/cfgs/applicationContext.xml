<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="stud" class="com.nt.beans.Student">
<constructor-arg type="java.lang.String" value="raja"/>
<constructor-arg type="int" value="101"/>
<constructor-arg type="float" value="95.55f"/>
</bean>

<bean id="mk" class="com.nt.beans.Marks">
<constructor-arg value="79" index="2"/>
<constructor-arg value="89" index="0"/>
<constructor-arg value="99" index="1"/>
</bean>


<bean id="emp" class="com.nt.beans.Employee">
<constructor-arg value="101" name="eno"/>
<constructor-arg value="999999" name="salary"/>
<constructor-arg value="CLERK" name="dsgn"/>
<constructor-arg value="10" name="deptNo"/>
<constructor-arg value="raja" name="ename"/>


</bean>

</beans>
